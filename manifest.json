{
  "name": "Dev.to global keyboard shortcuts",
  "version": "0.1.0",
  "description": "Global keyboard shortcuts for dev.to",
  "manifest_version": 2,
  "permissions": ["https://dev.to/*", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://dev.to/*"],
      "js": ["keyboard.js"],
      "css": ["keyboard.css"],
      "run_at": "document_start"
    }
  ]
}
