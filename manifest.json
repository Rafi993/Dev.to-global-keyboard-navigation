{
  "name": "Dev.to global keyboard navigation",
  "version": "0.0.1",
  "description": "Global keyboard navigation for dev.to",
  "manifest_version": 2,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://dev.to/*", "activeTab"],
      "js": ["keyboard.js"],
      "css": ["keyboard.css"],
      "run_at": "document_start"
    }
  ]
}
